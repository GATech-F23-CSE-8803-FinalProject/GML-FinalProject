{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Train DeepFRI",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/DeepFRI",
      "program": "./train_DeepFRI.py",
      "args": [
        "-gcd",
        "512",
        "512",
        "512",
        "-fcd",
        "1024",
        "-l2",
        "2e-5",
        "-lr",
        "0.0002",
        "--epochs",
        "1",
        "-ont",
        "mf",
        "--cmap_type",
        "ca",
        "--cmap_thresh",
        "10.0",
        // "--model_name",
        // "./results/DeepFRI-PDB_GraphConv_gcd_512-512-512_fcd_1024_ca_10.0_ec",
        "--train_tfrecord_fn",
        "./preprocessing/data/downloaded/PDB-GO/PDB_GO_train",
        "--valid_tfrecord_fn",
        "./preprocessing/data/downloaded/PDB-GO/PDB_GO_valid",
        "--annot_fn",
        "./preprocessing/data/nrPDB-GO_2019.06.18_annot.tsv",
        "--test_list",
        "./preprocessing/data/nrPDB-GO_2019.06.18_test.csv"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
